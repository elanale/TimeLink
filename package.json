{
  "name": "timelink",
  "version": "1.0.7",
  "description": "TimeLink â€“ Electron App with auto-update",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "dist": "electron-builder"
  },
  "build": {
    "appId": "com.elanale.timelink",
    "productName": "TimeLink",
    "mac": {
      "target": ["dmg", "zip"]
    },
    "win": {
      "target": "nsis"
    },
    "linux": {
      "target": ["AppImage", "deb"]
    },
    "publish": [
      {
        "provider": "github",
        "owner": "elanale",
        "repo": "TimeLink"
      }
    ]
  },
  "dependencies": {
    "electron-updater": "^6.6.2"
  },
  "devDependencies": {
    "electron": "^36.4.0",
    "electron-builder": "^26.0.12"
  }
}